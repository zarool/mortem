<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.Region?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>
<VBox alignment="CENTER" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity"
      prefHeight="400.0" prefWidth="600.0" stylesheets="@../style/start.css"
      xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="scenes.ControllerStart">
    <children>
        <Text id="text" fx:id="text" lineSpacing="4.0" text="" textAlignment="CENTER"/>
        <TextField id="input" fx:id="input" alignment="CENTER" disable="true" maxWidth="-Infinity" prefWidth="330.0"/>
        <Button id="startBtn" fx:id="loginBtn" onAction="#loginBtnClicked" mnemonicParsing="false" disable="true"
                text="Zacznij podróż..."/>
        <Region fx:id="region" visible="false"/>
    </children>
</VBox>
