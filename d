[32m+[m[32mpublic class Command {[m
[32m+[m
[32m+[m[32m    Hero hero;[m
[32m+[m[32m    MediaPlayer bgMusic;[m
[32m+[m[32m    int soundIndex = (int) (Math.random() * 2);[m
[32m+[m
[32m+[m[32m    Command(Hero hero) {[m
[32m+[m[32m        this.hero = hero;[m
[32m+[m
[32m+[m[32m        Media sound = new Media("file:///" + System.getProperty("user.dir").replace('\\', '/')[m
[32m+[m[32m                + "/src/assets/music/bgsound" + soundIndex + ".mp3");[m
[32m+[m[32m        bgMusic = new MediaPlayer(sound);[m
[32m+[m[32m        play(bgMusic);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    private void play(MediaPlayer music) {[m
[32m+[m[32m        music.play();[m
[32m+[m
[32m+[m[32m        music.setOnEndOfMedia(new Runnable() {[m
[32m+[m[32m            @Override[m
[32m+[m[32m            public void run() {[m
[32m+[m[32m                changeMusic();[m
[32m+[m[32m            }[m
[32m+[m[32m        });[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    private void changeMusic() {[m
[32m+[m[32m        bgMusic.stop();[m
[32m+[m
[32m+[m[32m        soundIndex = 1 - soundIndex;[m
[32m+[m[32m        Media sound2 = new Media("file:///" + System.getProperty("user.dir").replace('\\', '/')[m
[32m+[m[32m                + "/src/assets/music/bgsound" + soundIndex + ".mp3");[m
[32m+[m[32m        bgMusic = new MediaPlayer(sound2);[m
[32m+[m[32m        play(bgMusic);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    public String error(String msg) {[m
[32m+[m[32m        return "Å¹le wpisania komenda, popraw jÄ…, wojowniku";[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    public String gora(String msg) {[m
[32m+[m[32m        if (hero.hasDir("gora")) {[m
[32m+[m[32m            hero.setY(hero.getY() - 1);[m
[32m+[m[32m            return msg;[m
[32m+[m[32m        } else return error("");[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    public String prawo(String msg) {[m
[32m+[m[32m        if (hero.hasDir("prawo")) {[m
[32m+[m[32m            hero.setX(hero.getX() + 1);[m
[32m+[m[32m            return msg;[m
[32m+[m[32m        } else return error("");[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    public String dol(String msg) {[m
[32m+[m[32m        if (hero.hasDir("dol")) {[m
[32m+[m[32m            hero.setY(hero.getY() + 1);[m
[32m+[m[32m            return msg;[m
